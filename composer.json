{
    "name": "imedge/snmp",
    "description": "SNMP protocol implementation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Gelf",
            "email": "thomas@gelf.net"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "IMEdge\\Snmp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IMEdge\\Tests\\Snmp\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1.0",
        "ext-gmp": "*",
        "ext-json": "*",
        "gipfl/json": ">=0.2",
        "sop/asn1": "^4"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.4",
        "phpunit/php-code-coverage": "^10.0",
        "phpunit/phpunit": "^10",
        "squizlabs/php_codesniffer": "^3"
    }
}
