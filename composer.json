{
    "name": "imedge/snmp-packet",
    "description": "SNMP packet parser and renderer",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Gelf",
            "email": "thomas@gelf.net"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "IMEdge\\SnmpPacket\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IMEdge\\Tests\\SnmpPacket\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1.0",
        "ext-ctype": "*",
        "ext-gmp": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pcntl": "*",
        "freedsx/asn1": "^0.4.8",
        "imedge/json": ">=0.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.4",
        "phpunit/php-code-coverage": "^10.0",
        "phpunit/phpunit": "^10",
        "squizlabs/php_codesniffer": "^3"
    }
}
